@model Haberimdesin2.Models.ApplicationUser

@{
    
    ViewData["Title"] = "Edit";
    Layout = "_Layout";
}

<h2>Edit</h2>

<form asp-action="Edit">
    <div class="form-horizontal">
        <h4>ApplicationUser</h4>
        <hr />
        <div class="form-group">
            <label asp-for="Name" class="col-md-2 control-label">Adýnýz</label>
            <div class="col-md-10">
                <input asp-for="Name" class="form-control" />
                <span asp-validation-for="Name" class="text-danger" />
            </div>
        </div>
        <div class="form-group">
            <label asp-for="Surname" class="col-md-2 control-label">Soyadýnýz</label>
            <div class="col-md-10">
                <input asp-for="Surname" class="form-control" />
                <span asp-validation-for="Surname" class="text-danger" />
            </div>
        </div>
        <div class="form-group">
            <label asp-for="UserName" class="col-md-2 control-label">E-mail Adresiniz</label>
            <div class="col-md-10">
                <input asp-for="UserName" class="form-control" />
                <span asp-validation-for="UserName" class="text-danger" />
            </div>
        </div>
        <div  class="form-group">
            <div  class="col-md-offset-2 col-md-10">
                <input  type="submit" value="Save" class="btn btn-default" />
            </div>
            <div type="hidden" asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" type="hidden" asp-for="Id" />
            <div type="hidden" class="form-group">
               
                <div type="hidden" class="col-md-10">
                    <input type="hidden" asp-for="AccessFailedCount" class="form-control" />
                    <span type="hidden" asp-validation-for="AccessFailedCount" class="text-danger" />
                </div>
            </div>
            <div type="hidden" class="form-group">
               
                <div type="hidden" class="col-md-10">
                    <input type="hidden" asp-for="BirthDate" class="form-control" />
                    <span type="hidden" asp-validation-for="BirthDate" class="text-danger" />
                </div>
            </div>
            <div type="hidden" class="form-group">
             
                <div type="hidden" class="col-md-10">
                    <input type="hidden" asp-for="ConcurrencyStamp" class="form-control" />
                    <span type="hidden" asp-validation-for="ConcurrencyStamp" class="text-danger" />
                </div>
            </div>
            <div type="hidden" class="form-group">
              
                <div type="hidden" class="col-md-10">
                    <input type="hidden"asp-for="Email" class="form-control" />
                    <span type="hidden" asp-validation-for="Email" class="text-danger" />
                </div>
            </div>
            <div type="hidden" class="form-group">
                <div type="hidden"  class="col-md-offset-2 col-md-10">
                    <div type="hidden" class="checkbox">
                        <input type="hidden" asp-for="EmailConfirmed" />
                       
                    </div>
                </div>
            </div>
            <div type="hidden" class="form-group">
                
                <div type="hidden" class="col-md-10">
                    <input type="hidden" asp-for="Gender" class="form-control" />
                    <span type="hidden" asp-validation-for="Gender" class="text-danger" />
                </div>
            </div>
            <div type="hidden" class="form-group">
                <div type="hidden" class="col-md-offset-2 col-md-10">
                    <div class="checkbox">
                        <input type="hidden"  asp-for="LockoutEnabled" />
                       
                    </div>
                </div>
            </div>
            <div type="hidden" class="form-group">
              
                <div type="hidden" class="col-md-10">
                    <input type="hidden" asp-for="LockoutEnd" class="form-control" />
                    <span type="hidden" asp-validation-for="LockoutEnd" class="text-danger" />
                </div>
            </div>

            <div type="hidden"class="form-group">
               
                <div type="hidden" class="col-md-10">
                    <input type="hidden" asp-for="NormalizedEmail" class="form-control" />
                    <span type="hidden" asp-validation-for="NormalizedEmail" class="text-danger" />
                </div>
            </div>
            <div type="hidden" class="form-group">
           
                <div type="hidden" class="col-md-10">
                    <input type="hidden" asp-for="NormalizedUserName" class="form-control" />
                    <span type="hidden" asp-validation-for="NormalizedUserName" class="text-danger" />
                </div>
            </div>
            <div type="hidden" class="form-group">
             
                <div type="hidden" class="col-md-10">
                    <input type="hidden" asp-for="PasswordHash" class="form-control" />
                    <span type="hidden" asp-validation-for="PasswordHash" class="text-danger" />
                </div>
            </div>
            <div type="hidden" class="form-group">
               
                <div type="hidden" class="col-md-10">
                    <input type="hidden" asp-for="PhoneNumber" class="form-control" />
                    <span type="hidden" asp-validation-for="PhoneNumber" class="text-danger" />
                </div>
            </div>
            <div type="hidden" class="form-group">
                <div type="hidden" class="col-md-offset-2 col-md-10">
                    <div type="hidden"class="checkbox">
                        <input type="hidden" asp-for="PhoneNumberConfirmed" />
                      
                    </div>
                </div>
            </div>
            <div class="form-group">

                <div type="hidden" class="col-md-10">
                    <input type="hidden" asp-for="ProfileImgURL" class="form-control" />
                    <span type="hidden" asp-validation-for="ProfileImgURL" class="text-danger" />
                </div>
            </div>
            <div type="hidden" class="form-group">

                <div class="col-md-10">
                    <input type="hidden" asp-for="SecurityStamp" class="form-control" />
                    <span type="hidden" asp-validation-for="SecurityStamp" class="text-danger" />
                </div>
            </div>

            <div type="hidden" class="form-group">
                <div type="hidden" class="col-md-offset-2 col-md-10">
                    <div type="hidden" class="checkbox">
                        <input type="hidden" asp-for="TwoFactorEnabled" />

                    </div>
                </div>
            </div>


        </div>
    </div>
</form>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
